{
    email {env.HTTPS_CERT_EMAIL}
}

{env.HOST} {
    @rateLimited {
        path /api/*
        path /events/*
        rate_limit {
            window 1m
            bursts 600
        }
    }

    handle @rateLimited {
        reverse_proxy backend:{env.BPORT}
    }

    handle /* {
        reverse_proxy frontend:{env.FPORT}
    }
}